{
  "name": "__MSG_title__",
  "version": "4.0.0",
  "manifest_version": 3,
  "description": "__MSG_titleDesc__",
  "short_name": "BadiCal",
  "author": "Glen Little",
  "default_locale": "en",
  "icons": {
    "48": "badi19a-48.png",
    "128": "badi19a-128.png"
  },
  "background": {
    "service_worker": "service_worker.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google-analytics.com/mp/collect",
        "https://maker.ifttt.com/trigger/",
        "https://hooks.zapier.com/hooks/catch/"
      ],
      "css": [
        "empty.css"
      ]
    }
  ],
  "permissions": [
    "alarms",
    "storage",
    "notifications",
    "geolocation",
    "contextMenus",
    "clipboardWrite"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+D",
        "mac": "Command+Shift+D",
        "chromeos": "Ctrl+Shift+D",
        "linux": "Ctrl+Shift+D"
      }
    }
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "__MSG_browserActionTitle__",
    "default_icon": {
      "48": "badi19a-48.png"
    }
  }
}